{"name":"t:sections.header.name","type":"header","sections":{"announcement-bar":{"type":"announcement-bar","blocks":{"announcement-bar-0":{"type":"announcement","settings":{"text":"LAATSTE KANS: TOT 70% KORTING â€“ MIS HET NIET","link":""}},"announcement_tpHUGJ":{"type":"announcement","settings":{"text":"100% GELD TERUG GARANTIE","link":""}},"announcement_dcibYc":{"type":"announcement","disabled":true,"settings":{"text":"30 DAGEN ZORGELOOS RUILEN","link":""}}},"block_order":["announcement-bar-0","announcement_tpHUGJ","announcement_dcibYc"],"custom_css":[".utility-bar {height: 90%;}"],"settings":{"color_scheme":"scheme-77cb9eaa-2e8f-4c11-83fb-7dc39be798a0","show_line_separator":false,"show_social":false,"auto_rotate":true,"change_slides_speed":3,"enable_country_selector":false,"enable_language_selector":false}},"header":{"type":"header","custom_css":[".header__menu-item {font-size: 16px; text-transform: uppercase; letter-spacing: 0.2em;}","@media screen and (max-width: 767px) {.header__heading-logo {width: 175px !important; \/* Adjust the width as needed *\/ height: auto !important; \/* Maintain aspect ratio *\/ max-width: none !important; \/* Remove max-width constraints *\/ } .header__heading-logo-wrapper {display: flex; justify-content: center; align-items: center; }}"],"settings":{"logo_position":"top-center","menu":"main-menu","menu_type_desktop":"drawer","sticky_header_type":"on-scroll-up","show_line_separator":false,"color_scheme":"scheme-1","menu_color_scheme":"scheme-1","enable_country_selector":false,"enable_language_selector":false,"enable_customer_avatar":false,"mobile_logo_position":"center","margin_bottom":0,"padding_top":8,"padding_bottom":8}},"custom_liquid_pM6neH":{"type":"custom-liquid","disabled":true,"settings":{"custom_liquid":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Dynamic Countdown Timer<\/title>\n<style>\n  .custom-announcement-bar {\n    background-color: #1c1d1d !important;\n    color: #1B5340;\n    text-align: center;\n    padding: 7px 0;\n    font-family: Arial, sans-serif;\n    font-size: 12px;\n    font-weight: bold;\n  }\n  #timer {\n    color: #ffffff;\n  }\n<\/style>\n<\/head>\n<body>\n\n<div class=\"custom-announcement-bar\">\n  <span><span id=\"timer\">00:00:00<\/span> DE TIJD DRINGT VOOR ONZE GROOTSTE SALE!<\/span>\n<\/div>\n\n<script>\n  \/\/ Set the countdown duration in seconds (e.g., 3 hours, 0 minutes, and 0 seconds)\n  const countdownDuration = 9394; \/\/ 3 hours in seconds\n\n  function updateTimer() {\n    \/\/ Get the current timestamp\n    const now = new Date().getTime();\n\n    \/\/ Retrieve or set the end time in sessionStorage\n    let endTime = sessionStorage.getItem(\"endTime\");\n    if (!endTime) {\n      endTime = now + countdownDuration * 1000; \/\/ Calculate the new end time\n      sessionStorage.setItem(\"endTime\", endTime);\n    } else {\n      endTime = parseInt(endTime, 10); \/\/ Convert to a number\n    }\n\n    \/\/ Calculate the remaining time\n    const distance = endTime - now;\n\n    if (distance > 0) {\n      \/\/ Convert distance to hours, minutes, and seconds\n      const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) \/ (1000 * 60 * 60));\n      const minutes = Math.floor((distance % (1000 * 60 * 60)) \/ (1000 * 60));\n      const seconds = Math.floor((distance % (1000 * 60)) \/ 1000);\n\n      \/\/ Update the timer display\n      document.getElementById(\"timer\").innerHTML =\n        (hours < 10 ? \"0\" : \"\") + hours + \":\" +\n        (minutes < 10 ? \"0\" : \"\") + minutes + \":\" +\n        (seconds < 10 ? \"0\" : \"\") + seconds;\n    } else {\n      \/\/ If the timer ends, display \"EXPIRED\"\n      clearInterval(timerInterval);\n      document.getElementById(\"timer\").innerHTML = \"EXPIRED\";\n    }\n  }\n\n  \/\/ Update the timer every second\n  const timerInterval = setInterval(updateTimer, 1000);\n  updateTimer(); \/\/ Call immediately to avoid delay\n<\/script>\n\n<\/body>\n<\/html>","color_scheme":"","padding_top":0,"padding_bottom":0}}},"order":["announcement-bar","header","custom_liquid_pM6neH"]}